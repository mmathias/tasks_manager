<h1>Login#new</h1>

<%= form_tag "/login" do %>

  <p>
    <%= label_tag :credential, t("labels.login.credential") %>
    <%= text_field_tag :credential %>
  </p>
  <p>
    <%= label_tag :password, t("labels.login.password") %>
    <%= password_field_tag :password %>
  </p>
  <p>
    <%= submit_tag t("buttons.login")%>
  </p>
<% end %>
